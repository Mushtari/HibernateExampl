<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC  
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
          
<hibernate-mapping package="com.LTI.HibExamples.HibernateExmpl">
  <class name="Employ" table="Employ">
    	 <id name="id" column="id">
    	 <generator class="sequence">
    	  <param name="sequence">mysequence</param>
    	  </generator>
    	 </id>
   <property name="empName" column="empname"/>
   <property name="branch" column="branch"/>
  </class>
     </hibernate-mapping>